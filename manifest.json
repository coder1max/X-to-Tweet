{
  "manifest_version": 3,
  "name": "X to Tweet",
  "author": "coder.un.max@gmail.com",
  "version": "1.0",
  "description": "This extension replaces the X with the good old Twitter bird.",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["content_script.js"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
